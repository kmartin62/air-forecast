<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/home_text55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="329dp"
        android:layout_marginTop="75dp"
        android:textSize="35sp" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="676dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="55dp"
        android:background="@drawable/gradient">

        <FrameLayout
            android:id="@+id/frame_pm25"
            android:layout_width="218dp"
            android:layout_height="150dp"
            android:layout_marginStart="79dp"
            android:layout_marginTop="351dp">

            <TextView
                android:id="@+id/home_text3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:textColor="@color/common_google_signin_btn_text_dark_default"
                android:textSize="36sp" />

            <com.marcinmoskala.arcseekbar.ArcSeekBar
                android:id="@+id/seekBarArc123"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/frame_aqi"
            android:layout_width="218dp"
            android:layout_height="150dp"
            android:layout_marginStart="79dp"
            android:layout_marginTop="44dp">

            <TextView
                android:id="@+id/home_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:textColor="@color/common_google_signin_btn_text_dark_default"
                android:textSize="36sp" />

            <com.marcinmoskala.arcseekbar.ArcSeekBar
                android:id="@+id/seekBarArc1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true" />
        </FrameLayout>

        <TextView
            android:id="@+id/text_aqi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="40dp"
            android:textColor="@color/common_google_signin_btn_text_dark_default"
            android:textSize="25sp"
            android:visibility="visible" />

        <TextView
            android:id="@+id/text_pm10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="186dp"
            android:textColor="@color/common_google_signin_btn_text_dark_default"
            android:textSize="25sp"
            android:visibility="visible" />

        <TextView
            android:id="@+id/text_pm25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="347dp"
            android:textColor="@color/common_google_signin_btn_text_dark_default"
            android:textSize="25sp"
            android:visibility="visible" />

        <TextView
            android:id="@+id/infoText"
            android:layout_width="match_parent"
            android:layout_height="252dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="420dp" />

        <FrameLayout
            android:id="@+id/frame_pm10"
            android:layout_width="218dp"
            android:layout_height="150dp"
            android:layout_marginStart="79dp"
            android:layout_marginTop="190dp">

            <TextView
                android:id="@+id/home_text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:textColor="@color/common_google_signin_btn_text_dark_default"
                android:textSize="36sp" />

            <com.marcinmoskala.arcseekbar.ArcSeekBar
                android:id="@+id/seekBarArc12"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true" />
        </FrameLayout>
    </FrameLayout>
</RelativeLayout>